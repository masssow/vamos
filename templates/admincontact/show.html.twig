{% extends 'base-admin.html.twig' %}

{% block title %}Contact{% endblock %}

{% block body %}
    <h1>Contact</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ contact.email }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ contact.message }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admincontact_index') }}">back to list</a>

    <a href="{{ path('app_admincontact_edit', {'id': contact.id}) }}">edit</a>

    {{ include('admincontact/_delete_form.html.twig') }}
{% endblock %}
